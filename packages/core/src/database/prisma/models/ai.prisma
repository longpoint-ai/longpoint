model AiProviderConfig {
  providerId String
  config     Json?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt

  @@unique([providerId])
}

model Classifier {
  id          String   @id @default(cuid(2))
  name        String   @unique
  description String?
  modelId     String
  modelConfig Json?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@index([modelId])
}
